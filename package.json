        "import": "./dist/index.mjs",
        "require": "./dist/index.cjs"
    },
    "types": "dist/index.d.ts",
    "keywords": [
        "cache"
    ],
    "author": {
        "name": "Juri Adams",
        "email": "juri@adams.sh",
        "url": "https://adams.sh"
    },
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^20.5.9",
        "@vitest/coverage-v8": "^0.34.3",
        "bunchee": "^3.7.1",
        "typescript": "^5.2.2",
        "vitest": "^0.34.3"
    }
}
