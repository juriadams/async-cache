{
    "name": "@juriadams/async-cache",
    "version": "0.1.0",
    "description": "A supercharged caching utility for the edge.",
    "main": "./dist/index.mjs",
    "scripts": {
        "test": "vitest run --coverage",
        "prebuild": "rm -rf ./dist",
        "build": "bunchee -m --dts",
        "prepublish": "npm run build",
        "publish": "npm publish --access=public"
    },
    "files": [
        "./dist"
    ],
    "exports": {
        "module": "./dist/index.esm.js",
        "import": "./dist/index.mjs",
        "require": "./dist/index.cjs"
    },
    "types": "dist/index.d.ts",
    "keywords": [
        "cache"
    ],
    "author": {
        "name": "Juri Adams",
        "email": "juri@adams.sh",
        "url": "https://adams.sh"
    },
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^20.5.9",
        "@vitest/coverage-v8": "^0.34.3",
        "bunchee": "^3.7.1",
        "typescript": "^5.2.2",
        "vitest": "^0.34.3"
    }
}
