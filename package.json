{
    "name": "@juriadams/async-cache",
    "version": "1.0.0",
    "description": "An efficient caching utility with asynchronous capabilities for third-party retrieval and revalidation.",
    "main": "./dist/index.mjs",
    "scripts": {
        "test": "vitest run --coverage",
        "prebuild": "rm -rf ./dist",
        "build": "bunchee -m --dts"
